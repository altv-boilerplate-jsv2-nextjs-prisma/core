{
    "name": "core",
    "version": "0.0.1",
    "description": "",
    "scripts": {
        "[-] Server Deployment Commands (They All Do Different Things)": "",
        "build": "node ./scripts/compiler.js && node ./scripts/copy.js && node ./scripts/transform.js",
        "webview": "npm run build:webview && npm run webview:start",
        "webview:build": "next build ./src-webviews",
        "webview:start": "next start ./src-webviews",
        "webview:dev": "next dev ./src-webviews --hostname localhost --port 3000",
        "windows": "npm run build && altv-server.exe",
        "windows:start": "altv-server.exe",
        "linux": "npm run build && ./altv-server",
        "linux:start": "./altv-server",
        "dev": "node ./scripts/watch.js",
        "[-] Utility": "",
        "update": "altv-pkg release",
        "[-] Prisma": "",
        "prisma:push": "npx prisma db push",
        "prisma:migrate": "npx prisma migrate dev",
        "prisma:generate": "npx prisma generate"
    },
    "author": "DevPanda",
    "license": "ISC",
    "devDependencies": {
        "@altv/client": "^0.0.47",
        "@altv/natives": "^0.0.9",
        "@altv/server": "^0.0.48",
        "@altv/shared": "^0.0.24",
        "@altv/webview": "^0.0.6",
        "@swc/cli": "^0.4.0",
        "@swc/core": "^1.7.28",
        "@types/node": "^22.7.4",
        "esbuild": "^0.24.0",
        "fs-extra": "^11.2.0",
        "glob": "^11.0.0",
        "prisma": "^5.20.0",
        "typescript": "latest",
        "watcher": "^2.3.1"
    },
    "dependencies": {
        "@prisma/client": "^5.20.0",
        "@types/uuid": "^10.0.0",
        "altv-pkg": "^2.10.0",
        "prisma-json-types-generator": "^3.1.1",
        "toml": "^3.0.0",
        "uuid": "^11.0.2"
    },
    "prettier": {
        "tabWidth": 4,
        "semi": true,
        "printWidth": 120,
        "arrowParens": "always",
        "singleQuote": true
    },
    "engines": {
        "node": ">=22"
    },
    "type": "module"
}
